#! /usr/bin/env python
# -*- codes: utf-8 -*-
import sys
from clouseau.clouseau import Clouseau
from clouseau.clients.console import ConsoleClient 


def main():
    clouseau = Clouseau()
    client = ConsoleClient()
    sys.argv.pop(0)
    clouseau.main( _args=sys.argv, client=client )


if __name__ == '__main__':
    main()
