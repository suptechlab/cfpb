{
  "name": "cf-buttons",
  "version": "0.7.0",
  "description": "Button styles including default, secondary, destructive, disabled, super, and compound buttons, button links, buttons with icons, and button groups for Capital Framework.",
  "keywords": ["capital-framework", "capital", "buttons", "less"],
  "authors": [
    {
      "name": "Consumer Financial Protection Bureau",
      "url": "http://cfpb.github.io/"
    }
  ],
  "homepage": "https://github.com/cfpb/cf-buttons",
  "repository": {
    "type": "git",
    "url": "https://github.com/cfpb/cf-buttons.git"
  },
  "license": "https://github.com/cfpb/cf-buttons/blob/master/TERMS.md",
  "main": "src/cf-buttons.less",
  "dependencies": {
    "cf-colors": "git://github.com/cfpb/cf-colors.git",
    "cf-typography": "git://github.com/cfpb/cf-typography.git",
    "font-awesome": "~3.2.1"
  },
  "exportsOverride": {
    "cf-*": {
      "css": "src/**/*.css",
      "less": "src/**/*.less",
      "fonts": "src/fonts/*.*",
      "js": "src/**/*.js"
    },
    "font-awesome": {
      "css": "css/*.css",
      "fonts": "font/*.*"
    }
  }
}
