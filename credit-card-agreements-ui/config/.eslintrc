rules:
  camelcase:
    - 0
    -
      properties: never
  global-require: 0
  max-len: 0
  no-console: 0
  no-inline-comments: 0
  no-process-env: 0
  no-sync: 0
  no-undefined: 0
  require-jsdoc: 0

  array-bracket-spacing: 0
  comma-dangle: 0
  complexity: 0
  consistent-return: 0
  indent: 0
  indent-legacy: 0
  no-else-return: 0
  no-eq-null: 0
  no-multiple-empty-lines: 0
  no-shadow: 0
  no-trailing-spaces: 0
  no-warning-comments: 0
  operator-linebreak: 0
  prefer-const: 0
  quote-props: 0
  space-in-parens: 0
  template-curly-spacing: 0