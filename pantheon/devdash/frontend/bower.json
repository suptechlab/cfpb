{
  "name": "dev-dash",
  "version": "0.1.0",
  "description": "A Dashboard for Devs and Designers.",
  "keywords": [
    "open source"
  ],
  "authors": [
    {
      "name": "Consumer Financial Protection Bureau",
      "url": "http://cfpb.github.io/"
    }
  ],
  "homepage": "https://github.com/cfpb/dev-dash",
  "repository": {
    "type": "git",
    "url": "https://github.com/cfpb/dev-dash.git"
  },
  "license": "https://github.com/cfpb/dev-dash/blob/master/TERMS.md",
  "dependencies": {
    "angular": "~1.3.2",
    "cf-alerts": "git://github.com/himedlooff/cf-alerts.git#gh-pages",
    "cf-breadcrumbs": "git://github.com/himedlooff/cf-breadcrumbs.git#gh-pages",
    "cf-buttons": "~1.3.0",
    "cf-core": "~0.8.0",
    "cf-expandables": "~0.6.1",
    "cf-forms": "~1.1.0",
    "cf-grid": "~0.9.0",
    "cf-icons": "~0.4.0",
    "cf-layout": "~0.1.0",
    "cf-tabs": "git://github.com/himedlooff/cf-tabs.git#gh-pages",
    "html5shiv": "latest",
    "jquery": "~1.11.0",
    "jquery.easing": "~1.3.0"
  },
  "exportsOverride": {
    "cf-*": {
      "css": "src/**/*.css",
      "less": "src/**/*.less",
      "fonts": "src/fonts/*.*",
      "js": "src/**/*.js"
    },
    "html5shiv": {
      "js": "dist/html5shiv-printshiv.min.js"
    },
    "jquery.easing": {
      "js": "js/jquery.easing.js"
    }
  }
}
