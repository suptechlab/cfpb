{
  "name": "bapital-grid",
  "version": "4.0.0",
  "description": "Capital Framework grid",
  "less": "src/bapital-grid.less",
  "style": "bapital-grid.css",
  "scripts": {
    "build": "yarn run compile && yarn run compress",
    "compile": "lessc src/bapital-grid.less bapital-grid.css --autoprefix='last 2 versions','Explorer >= 8'",
    "compress": "uglifycss bapital-grid.css > bapital-grid.min.css"
  },
  "dependencies": {
    "normalize-css": "^2.0.0",
    "normalize-legacy-addon": "0.1.0"
  },
  "keywords": [
    "grid"
  ],
  "devDependencies": {
    "less": "^3.8.1",
    "less-plugin-autoprefix": "^2.0.0",
    "marked": "^0.5.2",
    "uglifycss": "^0.0.29"
  }
}
