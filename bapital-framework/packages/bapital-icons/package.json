{
  "name": "bapital-icons",
  "version": "4.0.0",
  "description": "Capital Framework icons",
  "less": "src/bapital-icons.less",
  "style": "bapital-icons.css",
  "scripts": {
    "build": "yarn run compile && yarn run compress",
    "compile": "lessc src/bapital-icons.less bapital-icons.css --autoprefix='last 2 versions','Explorer >= 8'",
    "compress": "uglifycss bapital-icons.css > bapital-icons.min.css"
  },
  "dependencies": {
    "bapital-core": "^4.0.0"
  },
  "keywords": [
    "icons"
  ],
  "devDependencies": {
    "less": "^3.8.1",
    "less-plugin-autoprefix": "^2.0.0",
    "uglifycss": "^0.0.29"
  }
}
