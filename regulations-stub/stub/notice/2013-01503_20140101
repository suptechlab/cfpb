                            "children": [], 
                            "footnote_refs": [
                                {
                                    "offset": 713, 
                                    "paragraph": 2, 
                                    "reference": "84"
                                }, 
                                {
                                    "offset": 768, 
                                    "paragraph": 6, 
                                    "reference": "85"
                                }, 
                                {
                                    "offset": 1523, 
                                    "paragraph": 8, 
                                    "reference": "86"
                                }
