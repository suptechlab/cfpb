                {
                    "children": [
                        {
                            "children": [], 
                            "footnote_refs": [
                                {
                                    "offset": 459, 
                                    "paragraph": 1, 
                                    "reference": "130"
                                }, 
                                {
                                    "offset": 846, 
                                    "paragraph": 1, 
                                    "reference": "131"
