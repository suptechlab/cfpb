{
  "name": "@cfpb/rental-assistance-finder",
  "version": "0.6.0",
  "scripts": {
    "start": "react-scripts start",
    "build": "node ./build-non-split.js",
    "prepack": "cp build/static/js/main.js dist/ && cp build/static/css/main.*.css dist/",
