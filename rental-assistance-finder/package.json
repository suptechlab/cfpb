    "production": [
      "last 2 versions",
      "> 0.5%",
      "ie >= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cypress/react": "^5.10.3",
    "@cypress/webpack-dev-server": "^1.7.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "cypress": "^9.1.1",
    "cypress-react-selector": "^2.3.11",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-react": "^7.27.1",
    "got": "11.8.2",
    "i18next": "^20.4.0",
    "jest-fetch-mock": "3.0.3",
    "polyfill-array-includes": "^2.0.0",
    "promise-polyfill": "8.2.0",
    "react": "^17.0.2",
    "react-app-polyfill": "^2.0.0",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.11.4",
    "react-scripts": "4.0.3",
    "react-select": "4.3.1",
    "rewire": "5.0.0",
    "symbol-es6": "^0.1.2",
    "web-vitals": "^1.0.1",
    "whatwg-fetch": "^3.6.2"
  },
  "description": "This tool allows users to view and filter emergency rental assistance programs funded by the U.S. Department of Treasuryâ€™s Emergency Rental Assistance Program.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cfpb/rental-assistance-finder.git"
  },
  "keywords": [
    "CFPB"
  ],
  "author": "CFPB",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/cfpb/rental-assistance-finder/issues"
  },
  "homepage": "https://github.com/cfpb/rental-assistance-finder",
  "jest": {
    "resetMocks": false,
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)"
    ]
  }
}
