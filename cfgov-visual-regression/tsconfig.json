{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "lib": ["ES2021.String"],
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "types": ["cypress", "node"]
  },
  "include": ["**/*.ts", "**/*.json"]
}
