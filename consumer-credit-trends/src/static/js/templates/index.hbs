<body>

<!--
    =================
    Begin page source
    =================
-->

<header class="wrapper wrapper__match-content" role="banner">
    <div class="block block__sub">
        <h1>Credit market trends: charts</h1>
    </div>
</header>

<main class="content" id="main" role="main">
    <div class="content_bar"></div>
    <div class="content_wrapper">
        <div class="content_main">

<ol>
    {{#each charts}}
    <li><a href="/charts/{{ market }}/{{ reportType }}/{{ elementID }}.html">{{ elementID }}</a></li>
    {{/each}}
</ol>

{{#each charts}}
<div style="border-bottom: 4px solid gray">
    <h4>{{ market }}, {{ reportType }}, {{chartType}}</h4>
    <h5>{{ figureID }}</h5>
    {{> svg indexPage=true }}
</div>
{{/each}}

        
        </div><!-- END .content_main -->
    </div>
</main>

<footer role="contentinfo">
    <!-- Footer stuff -->
</footer>

<!--
    ============
    BODY SCRIPTS
    ============
    The number of scripts here must be kept to a minimum.
    Unless adding a significant amount of JS that is specific to a single page
    or section of the site, all custom JS should be added to src/static/js/app.js.
-->

<script src="/static/js/main.min.js"></script>

</body>
