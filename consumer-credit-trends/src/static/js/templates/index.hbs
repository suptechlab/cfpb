    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

<!--
    ==================
    PAGE-SPECIFIC META
    ==================
-->

    <title>Credit market trends: charts</title>
    <meta name="description" content="Generated d3 charts to show credit market trends.">

<!--
    ======
    STYLES
